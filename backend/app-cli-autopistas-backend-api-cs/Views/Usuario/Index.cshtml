@model IEnumerable<Project.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StrNombreUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrApellidoUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrSeudonimo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnmTipoUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrContrasena)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrCargoUsuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StrNombreUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrApellidoUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrSeudonimo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EnmTipoUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrContrasena)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrCargoUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IntCedulaUsuario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IntCedulaUsuario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntCedulaUsuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
