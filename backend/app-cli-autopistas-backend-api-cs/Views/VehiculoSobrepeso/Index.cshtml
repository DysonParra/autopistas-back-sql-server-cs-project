@model IEnumerable<Project.Models.VehiculoSobrepeso>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IntPesoMaximo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntDiferenciaPeso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrPlacaVehiculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BitBorrado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdDinamica)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IntPesoMaximo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntDiferenciaPeso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrPlacaVehiculo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BitBorrado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdDinamica)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IntIdRepeso">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IntIdRepeso">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntIdRepeso">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
